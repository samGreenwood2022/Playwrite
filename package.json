{
  "name": "playwrite",
  "version": "1.0.0",
  "description": "Playwrite project",
  "main": "dist/index.js",
  "scripts": {
    "test-ui": "npx playwright test --ui --reporter=html",
    "test-headless": "npx playwright test --project=chromium --reporter=html",
    "codegen": "npx playwright codegen https://source.thenbs.com/",
    "test:twice": "npx playwright test || npx playwright test",
    "cucumber": "cucumber-js --require-module ts-node/register --require tests/step_definitions/*.ts tests/features/*.feature"
  },
  "author": "S Greenwood",
  "license": "ISC",
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cucumber/cucumber": "^12.0.0",
    "@playwright/test": "^1.50.1",
    "@types/node": "^22.13.1",
    "axe-html-reporter": "^2.2.11",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "compilerOptions": {
    "outDir": "dist",
    "allowImportingTsExtensions": true,
    "module": "commonjs",
    "target": "es6",
    "strict": true
  }
}
